{
  "name": "babel-plugin-better-import",
  "version": "0.0.0-development",
  "description": "Babel plugin to transform import() into its Universal counterpart",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastian-software/babel-plugin-better-import.git"
  },
  "bugs": {
    "url": "https://github.com/sebastian-software/babel-plugin-better-import/issues"
  },
  "homepage": "https://github.com/sebastian-software/babel-plugin-better-import#readme",
  "author": "James Gillmore james@sebastian-software.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "eslint --fix ./",
    "format": "prettier --write '{src,__tests__,__test-helpers__}/**/*.js' && npm run lint",
    "precommit": "lint-staged && npm test",
    "cm": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.46",
    "@babel/preset-es2015": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/preset-stage-2": "^7.0.0-beta.46",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-tester": "^5.0.0",
    "babylon-options": "^2.0.1",
    "eslint": "^4.19.1",
    "eslint-config-readable": "^1.12.4",
    "flow-bin": "^0.72.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.4.0",
    "travis-github-status": "^1.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ]
    }
  },
  "dependencies": {
    "@babel/helper-module-imports": "^7.0.0-beta.46"
  }
}
